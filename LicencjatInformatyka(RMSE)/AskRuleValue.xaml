<Window x:Class="LicencjatInformatyka_RMSE_.AskRuleValue"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:newFolder1="clr-namespace:LicencjatInformatyka_RMSE_.NewFolder1"
        mc:Ignorable="d"
        Title="AskRuleValue" Height="300" Width="300" 
         d:DataContext="{d:DesignInstance newFolder1:ViewModel}">
    <Grid>
        <Label Content="Wrpowadz"  HorizontalAlignment="Left" Margin="10,22,0,0" VerticalAlignment="Top" Width="235"/>
        <Label Content="{Binding}" HorizontalAlignment="Left" Height="26" Margin="10,53,0,0" VerticalAlignment="Top" Width="235"/>
        <Button Command="{Binding ValueTrue}" Content="Prawda" HorizontalAlignment="Left" Height="28" Margin="10,212,0,0" VerticalAlignment="Top" Width="125"/>
        <Button Command="{Binding ValueUnknown}" Content="Nieznany" HorizontalAlignment="Left" Height="28" Margin="163,212,0,0" VerticalAlignment="Top" Width="100"/>

    </Grid>
</Window>
